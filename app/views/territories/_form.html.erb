<%= render partial: '/form_errors', locals: { model: territory } %>

<%= simple_form_for(territory) do |f| %>
  <%= f.input :name %>
  <% if current_user.master? %>
    <%= f.input :user_id, as: :radio_buttons, collection: user_collection_for_select(@territory) %>
  <% end %>
  <%= f.submit class: 'btn btn-success' %>
<% end %>
